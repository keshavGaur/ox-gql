{
  "name": "ox-graphql",
  "version": "1.0.0",
  "description": "GraphQL end point to serve request from Web clients",
  "main": "src/index.js",
  "preinstall": "npm install -g pm2@latest",
  "scripts": {
    "test": "time mocha",
    "start": "node src/index.js",
    "start:pm2": "pm2 src/index.js",
    "start:dev": "nodemon -w src src/index.js",
    "start:preprod": "pm2 start pm2/ecosystem.config.js --only mosaic-mylib-preprod:4080",
    "start:production": "pm2 start pm2/ecosystem.config.js --only mosaic-mylib-production",
    "start:staging": "pm2 start pm2/ecosystem.config.js --only mosaic-mylib-staging:4080",
    "start:dev-noimosaic": "pm2 start pm2/ecosystem.config.js --only mosaic-mylib-dev:4080",
    "start:artemis": "pm2 start pm2/ecosystem.config.js --only mosaic-mylib-artemis:4080",
    "lint": "eslint src tests/ut",
    "lintreport": "eslint -o lintreport.json -f json src tests/ut",
    "lint-fix": "eslint --fix src",
    "coverage": "nyc npm test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-datasource": "^0.1.3",
    "apollo-datasource-rest": "^0.1.5",
    "apollo-server": "^2.16.1",
    "apollo-server-testing": "^2.16.1",
    "async": "^3.2.0",
    "config": "^3.2.4",
    "dataloader": "^1.4.0",
    "dotenv": "^6.1.0",
    "graphql": "^14.2.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.2.1",
    "graphql-tools": "^4.0.6",
    "lodash": "^4.17.19",
    "md5": "^2.3.0",
    "qs": "^6.9.1",
    "ramda": "^0.26.1",
    "request": "^2.88.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^8.2.1",
    "nock": "^10.0.2",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0"
  }
}
